include "/usr/share/themes/Industrial/gtk-2.0/gtkrc"


# This determines the font:

style "default_wmjump"
{
  font_name="DejaVu Sans 10"
  GtkButton::focus-line-width = 0
}
class "GtkWidget" style "default_wmjump"


# This determines the default color of the button:

style "other_buttons"
{
bg[NORMAL]="#FFD39B"
bg[PRELIGHT]="#FFD39B"
}
class "GtkButton" style "other_buttons" 

# This determines the default color of the message area:
style "messagearea_style"
{
  font_name="DejaVu Sans 14"
  base[NORMAL]="#F0F8FF"
}
class "GtkTextView" style "messagearea_style"

# This determines the background color of the wmjump window:

style "background_window_style"
{
  bg[NORMAL] = "#00ccff"
}
widget "*main_window" style "background_window_style"

style "background_window_currentonly_style"
{
  bg[NORMAL] = "#ccff00"
}
widget "*main_window_currentonly" style "background_window_currentonly_style"

# Here we define the color of a button corresponding 
# to the Tk applications:

style "mytk"
{
bg[NORMAL] = "#99ccff"
bg[PRELIGHT] = "#99ccff"
}
widget "*tk" style "mytk"
widget "*tk #*" style "mytk"

# Here we define the color of a button corresponding 
# to the applications "gsview" and "acroread":

style "mypink"
{
bg[NORMAL] = "#FF9eaa"
bg[PRELIGHT] = "#FF9eaa"
}
widget "*acroread" style "mypink"
widget "*gsview" style "mypink"

# note that "gsview" was the name of the window class
# note the asterisk * in "*gsview"; that asterisk is important !

# let us proceed with other application classes;
# let us define also the color of letters:

